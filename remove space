#include<bits/stdc++.h>
using namespace std;
void removevowel(string S){
   int n=S.size();
    string s1="";
   for(int i=0;i<n;i++){
       if(S[i]==' '){
           continue;
       }
       else{
           s1+=S[i];
       }
   }
    cout<<s1;
}
int main(){
    string s;
    cout<<"enter the string"<<endl;
    getline(cin,s);
    removevowel(s);
    
}
