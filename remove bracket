#include<bits/stdc++.h>
using namespace std;
void twosolve(string s){
    int n=s.size();
    string S="";
    for(int i=0;i<n;i++){
        if(s[i]=='('||s[i]==')'){
            continue;
        }
        S+=s[i];
    }
    cout<<S;
}
int main(){
    string s1;
    getline(cin,s1);
    twosolve(s1);
}
